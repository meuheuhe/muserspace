<!--my favorite-->
<div class="row row_style first_row">
 <div class="row-header">
    <h1 class="page-header header_style">
        My 좋아요
    </h1>
    <div role="tabpanel">
          <!-- Nav tabs -->
          <ul class="nav nav-tabs nav-tabs_style" role="tablist">
            <li role="presentation" class="active"><a href="#like_create" aria-controls="like_create" role="tab" data-toggle="tab">창작</a></li>
            <li role="presentation"><a href="#like_cover" aria-controls="like_cover" role="tab" data-toggle="tab">커버</a></li>
            <li role="presentation"><a href="#like_show" aria-controls="like_show" role="tab" data-toggle="tab">공연</a></li>
          </ul>
        </div>
 </div>
</div>

<div class="row history-row">
    <div class="history-profile-wrap">
        
        <div class="tab-content new-music-content">
            <div role="tabpanel" class="tab-pane active" id="like_create">
                <div class="row list">
                  <% for num in 1..8 %>
                    <div class="col-6 col-md-4 col-lg-3">
                      <div class="list-wrap">
                        <div class="thumbnail">
                          <a href="javascript:popupOpen();"><img src="http://via.placeholder.com/170x170" alt="앨범이미지"></a>
                          <div class="caption">
                            <p><a href="#" class="list-title">제목</a></p>
                          </div>
                        </div>
                      </div>
                    </div>
                  <% end %>
               </div>
            </div>
            <div role="tabpanel" class="tab-pane" id="like_cover">
                <div class="row list">
                  <% for num in 1..8 %>
                    <div class="col-6 col-md-6 col-lg-4">
                      <div class="list-wrap show-list-wrap">
                          <div class="thumbnail">
                              <div class="list-show">
                                <ul class="list-unstyled video-list-thumbs row">
                                	<li class="col-12">
                                		<a href="" title="Claudio Bravo, antes su debut con el Barça en la Liga">
                                			<img src="/assets/thumbimg.jpg" alt="Barca" class="img-responsive" height="130px" />
                                			<span class="glyphicon glyphicon-play"></span>
                                			<span class="duration">03:15</span>
                                		</a>
                                	</li>
                                </ul>
                              </div>
                            <div class="caption">
                              <p><a href="#" class="list-title">제목</a></p>
                            </div>
                          </div> 
                        </div>
                      </div>
                    <!--</div>-->
                  <% end %>
               </div>
            </div>
            <div role="tabpanel" class="tab-pane" id="like_show">
                <div class="row list">
                  <% for num in 1..8 %>
                    <div class="col-6 col-md-6 col-lg-4">
                      <div class="list-wrap show-list-wrap">
                          <div class="thumbnail">
                              <div class="list-show">
                                <ul class="list-unstyled video-list-thumbs row">
                                	<li class="col-12">
                                		<a href="" title="Claudio Bravo, antes su debut con el Barça en la Liga">
                                			<img src="/assets/thumbimg.jpg" alt="Barca" class="img-responsive" height="130px" />
                                			<span class="glyphicon glyphicon-play"></span>
                                			<span class="duration">03:15</span>
                                		</a>
                                	</li>
                                </ul>
                              </div>
                            <div class="caption">
                              <p><a href="#" class="list-title">제목</a></p>
                            </div>
                          </div> 
                        </div>
                      </div>
                    <!--</div>-->
                  <% end %>
               </div>
            </div>
        </div>
        
    </div>
</div>