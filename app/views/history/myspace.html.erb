<div class="row history-row first_row">
    <div class="history-profile-wrap">
        <div class="history-profile-img"><img class="img-circle" src='http://via.placeholder.com/150x150' alt='프로필이미지'></div>
        <div class="history-profile-content">
            <div class="content-1">
                <h1 class="history-profile-nickname">UserNickname</h1>
                <a href="/users/edit" class="btn btn-default history-profile-edit">프로필 편집</a>
            </div>
            <ul class="content-2">
                <li>게시물&nbsp;<span>10</span></li>
                <li><a href="">구독한수&nbsp;<span>10</span></a></li>
                <li><a href="">구독해준수&nbsp;<span>10</span></a></li>
            </ul>
            <div class="content-3">자기소개</div>
        </div>
    </div>
</div>

<div class="row row_style">
 <div class="row-header">
    <h1 class="page-header header_style">
        My 스페이스
    </h1>
    <div role="tabpanel">
      <!-- Nav tabs -->
      <ul class="nav nav-tabs nav-tabs_style" role="tablist">
        <li role="presentation" class="active"><a href="#create" aria-controls="home" role="tab" data-toggle="tab">창작</a></li>
        <li role="presentation"><a href="#cover" aria-controls="profile" role="tab" data-toggle="tab">커버</a></li>
        <li role="presentation"><a href="#show" aria-controls="messages" role="tab" data-toggle="tab">공연</a></li>
      </ul>
    </div>
 </div>
</div>

<div class="row history-row">
    <div class="history-profile-wrap">
        
        <div class="tab-content new-music-content">
            <div role="tabpanel" class="tab-pane active" id="create">
                <div class="row list">
                  <% for num in 1..8 %>
                    <div class="col-6 col-md-4 col-lg-3">
                      <div class="list-wrap">
                        <div class="thumbnail">
                          <a href="javascript:popupOpen();"><img src="http://via.placeholder.com/170x170" alt="앨범이미지"></a>
                          <div class="caption">
                            <p><a href="#" class="list-title">제목</a></p>
                          </div>
                        </div>
                      </div>
                    </div>
                  <% end %>
               </div>
            </div>
            <div role="tabpanel" class="tab-pane" id="cover">
                <div class="row list">
                  <% for num in 1..8 %>
                    <div class="col-6 col-md-6 col-lg-4">
                      <div class="list-wrap show-list-wrap">
                          <div class="thumbnail">
                              <div class="list-show">
                                <ul class="list-unstyled video-list-thumbs row">
                                	<li class="col-12">
                                		<a href="" title="Claudio Bravo, antes su debut con el Barça en la Liga">
                                			<img src="/assets/thumbimg.jpg" alt="Barca" class="img-responsive" height="130px" />
                                			<span class="glyphicon glyphicon-play"></span>
                                			<span class="duration">03:15</span>
                                		</a>
                                	</li>
                                </ul>
                              </div>
                            <div class="caption">
                              <p><a href="#" class="list-title">제목</a></p>
                            </div>
                          </div> 
                        </div>
                      </div>
                    <!--</div>-->
                  <% end %>
               </div>
            </div>
            <div role="tabpanel" class="tab-pane" id="show">
                <div class="row list">
                  <% for num in 1..8 %>
                    <div class="col-6 col-md-6 col-lg-4">
                      <div class="list-wrap show-list-wrap">
                          <div class="thumbnail">
                              <div class="list-show">
                                <ul class="list-unstyled video-list-thumbs row">
                                	<li class="col-12">
                                		<a href="" title="Claudio Bravo, antes su debut con el Barça en la Liga">
                                			<img src="/assets/thumbimg.jpg" alt="Barca" class="img-responsive" height="130px" />
                                			<span class="glyphicon glyphicon-play"></span>
                                			<span class="duration">03:15</span>
                                		</a>
                                	</li>
                                </ul>
                              </div>
                            <div class="caption">
                              <p><a href="#" class="list-title">제목</a></p>
                            </div>
                          </div> 
                        </div>
                      </div>
                    <!--</div>-->
                  <% end %>
               </div>
            </div>
        </div>
        
    </div>
</div>